    "chai": "^4.0.0",
    "colors": "^1.1.2",
    "mocha": "^4.0.0",
    "mocha-autotest": "^1.0.3",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
